{
  "name": "nav-ally",
  "version": "0.10.0",
  "description": "An automation tool for testing frontend applications against a set of web accessibility guidelines.",
  "main": "bin/validator.js",
  "scripts": {
    "start": "mocha src/test/run.js",
    "start-http": "http-server -p 7777 src/test/testweb",
    "test": "start-server-and-test start-http http://localhost:7777/ start-test",
    "serve-doc": "bundle exec jekyll serve --source ./docs --destination ./docs/_site --config ./docs/_config.yml",
    "start-test": "node src/test/validator-test.js",
    "lint": "nps lint",
    "reformat": "nps reformat",
    "docs": "nps docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/nav-ally.git"
  },
  "author": {
    "name": "Dervis Mansuroglu",
    "url": "https://github.com/dervism"
  },
  "license": "MIT",
  "bin": {
    "mocha": "bin/validator.js"
  },
  "bugs": {
    "url": "https://github.com/navikt/nav-ally/issues"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 2.15.11"
  },
  "files": [
    "bin",
    "src",
    "driver",
    "lib",
    "index.js",
    "run.sh"
  ],
  "homepage": "https://github.com/navikt/nav-ally",
  "keywords": [
    "wcag",
    "test",
    "accessibility",
    "mocha"
  ],
  "dependencies": {
    "axe-core": "3.1.2",
    "axe-webdriverjs": "2.0.1",
    "chai": "4.2.0",
    "http-server": "0.11.1",
    "js-yaml": "3.12.0",
    "mocha": "4.0.1",
    "node-fetch": "2.2.0",
    "selenium-webdriver": "4.0.0-alpha.1",
    "start-server-and-test": "1.7.4",
    "commander": "2.19.0"
  },
  "devDependencies": {
    "eslint": "4.8.0",
    "eslint-config-semistandard": "11.0.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.2.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-plugin-prettier": "2.6.0",
    "jsdoc": "3.5.5",
    "prettier-eslint-cli": "4.7.1",
    "markdownlint-cli": "0.9.0",
    "docdash": "1.0.0",
    "nps": "5.7.1",
    "rimraf": "2.6.2",
    "markdown-toc": "1.2.0",
    "mkdirp": "0.5.1"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false
  }
}

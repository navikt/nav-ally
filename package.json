{
  "name": "nav-ally",
  "version": "1.3.1",
  "description": "An automation tool for testing frontend applications against a set of web accessibility guidelines.",
  "main": "bin/validator.js",
  "scripts": {
    "start": "node bin/validator.js",
    "test": "mocha ./src/test/validator/**/*.js",
    "test-validation": "start-server-and-test start-http http://localhost:7777/ start-validationtest",
    "start-http": "serve -l 7777 src/test/validation",
    "start-validationtest": "node src/test/validation/validation-test.js",
    "serve-doc": "bundle exec jekyll serve --source ./docs --destination ./docs/_site --config ./docs/_config.yml",
    "lint": "nps lint",
    "lintfix": "nps lint.codefix",
    "reformat": "nps reformat",
    "docs": "nps docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/nav-ally.git"
  },
  "author": {
    "name": "Dervis Mansuroglu",
    "url": "https://github.com/dervism"
  },
  "license": "MIT",
  "bin": {
    "nav-ally": "bin/validator.js"
  },
  "bugs": {
    "url": "https://github.com/navikt/nav-ally/issues"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 2.15.11"
  },
  "files": [
    "bin",
    "src"
  ],
  "homepage": "https://github.com/navikt/nav-ally",
  "keywords": [
    "wcag",
    "test",
    "accessibility",
    "mocha"
  ],
  "dependencies": {
    "axe-core": "3.2.2",
    "axe-webdriverjs": "2.2.0",
    "commander": "2.20.0",
    "js-yaml": "^3.13.1",
    "lodash": "4.17.11",
    "mocha": "6.1.4",
    "node-fetch": "2.6.0",
    "selenium-webdriver": "4.0.0-alpha.1",
    "start-server-and-test": "1.9.1"
  },
  "devDependencies": {
    "docdash": "1.1.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-semistandard": "13.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "jsdoc": "^3.6.2",
    "mkdirp": "0.5.1",
    "nps": "5.9.5",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "2.6.3",
    "serve": "^11.0.1"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false
  }
}

{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": "./src",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/docdash",
    "destination": "docs/api",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  }
}